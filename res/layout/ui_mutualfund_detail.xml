<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:id="@+id/li_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_default2"
    android:orientation="vertical"
    tools:context="com.app.bids.UiWatchlistDetail" >

    <LinearLayout
        android:id="@+id/li_vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.95"
        android:orientation="vertical"
        android:visibility="visible" >

        <LinearLayout
            android:id="@+id/li_menu_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/tab_title_height"
                android:gravity="center_vertical"
                android:padding="@dimen/padding_default" >

                <LinearLayout
                    android:id="@+id/li_back"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center" >

                    <ImageView
                        android:id="@+id/img_home_dropdown"
                        android:layout_width="@dimen/img_title_wh"
                        android:layout_height="@dimen/img_title_wh"
                        android:src="@drawable/icon_back" />

                    <com.app.custom.CustomTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="Mutual Fund"
                        android:textColor="@color/c_content"
                        android:textSize="@dimen/f_description" />
                </LinearLayout>

                <com.app.custom.CustomTextView
                    android:id="@+id/tv_title_symbol"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.95"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="&apos;s Detail"
                    android:textColor="@color/c_content2"
                    android:textSize="@dimen/f_normalmor"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/img_portfolio_add"
                    android:layout_width="@dimen/img_back"
                    android:layout_height="@dimen/img_back"
                    android:background="@drawable/icon_portfolio_add"
                    android:visibility="visible" />

                <ImageView
                    android:id="@+id/img_follow"
                    android:layout_width="@dimen/add_symbol"
                    android:layout_height="@dimen/add_symbol"
                    android:background="@drawable/icon_favorite_default"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/img_setalert"
                    android:layout_width="@dimen/add_symbol"
                    android:layout_height="@dimen/add_symbol"
                    android:layout_marginLeft="@dimen/padding_default"
                    android:background="@drawable/menu_bottom_setalert"
                    android:visibility="gone" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_wh"
                android:background="@color/c_line" />

            <LinearLayout
                android:id="@+id/li_tab_search"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone" >

                <LinearLayout
                    android:id="@+id/li_search"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/bg_border_search"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="@dimen/padding_default" >

                        <EditText
                            android:id="@+id/et_search"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.95"
                            android:background="@drawable/bg_input"
                            android:drawableLeft="@drawable/icon_search"
                            android:ems="10"
                            android:hint="Symbol"
                            android:textColor="@color/c_title"
                            android:textColorHint="@color/c_title"
                            android:textSize="@dimen/f_normal" >

                            <requestFocus />
                        </EditText>

                        <com.app.custom.CustomTextView
                            android:id="@+id/tv_close_search"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/padding_default"
                            android:layout_marginRight="@dimen/padding_row"
                            android:gravity="center"
                            android:text="Cancle"
                            android:textColor="@color/c_content"
                            android:textSize="@dimen/f_symbol_detail" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/padding_default"
                        android:paddingRight="@dimen/padding_default"
                        android:paddingTop="@dimen/padding_row"
                        android:visibility="visible" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/border_search_default"
                            android:orientation="horizontal"
                            android:weightSum="3" >

                            <com.app.custom.CustomTextView
                                android:id="@+id/tv_search_common"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/border_search_activeleft"
                                android:gravity="center"
                                android:padding="@dimen/padding_default"
                                android:text="Common"
                                android:textColor="@color/bg_default"
                                android:textSize="@dimen/f_symbol_detail" />

                            <View
                                android:layout_width="@dimen/search_border_line"
                                android:layout_height="match_parent"
                                android:background="@color/bg_search_select" />

                            <com.app.custom.CustomTextView
                                android:id="@+id/tv_search_warrant"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="@dimen/padding_default"
                                android:text="Warrant"
                                android:textColor="@color/c_content"
                                android:textSize="@dimen/f_symbol_detail" />

                            <View
                                android:layout_width="@dimen/search_border_line"
                                android:layout_height="match_parent"
                                android:background="@color/bg_search_select" />

                            <com.app.custom.CustomTextView
                                android:id="@+id/tv_search_dw"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="@dimen/padding_default"
                                android:text="DW"
                                android:textColor="@color/c_content"
                                android:textSize="@dimen/f_symbol_detail" />
                        </LinearLayout>

                        <ListView
                            android:id="@+id/list_search_symbol"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" >
                        </ListView>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/li_search_tabbegin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bg_border_search"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/padding_default"
                    android:paddingLeft="@dimen/padding_default"
                    android:paddingTop="@dimen/padding_default"
                    android:visibility="visible" >

                    <ImageView
                        android:layout_width="@dimen/img_icon"
                        android:layout_height="@dimen/img_icon"
                        android:layout_marginRight="@dimen/padding_default"
                        android:src="@drawable/ic_launcher" />

                    <LinearLayout
                        android:id="@+id/li_search_select"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/padding_default"
                        android:background="@drawable/bg_search_select"
                        android:gravity="center"
                        android:visibility="gone" >

                        <ImageView
                            android:layout_width="@dimen/img_icon_search"
                            android:layout_height="@dimen/img_icon_search"
                            android:background="@drawable/icon_search"
                            android:visibility="visible" />

                        <com.app.custom.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="Symbol"
                            android:textColor="@color/c_title"
                            android:textSize="@dimen/f_symbol_detail"
                            android:visibility="visible" />
                    </LinearLayout>

                    <com.app.custom.CustomTextViewSliding
                        android:id="@+id/tv_sliding_marquee"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.95"
                        android:background="@drawable/bg_sliding_marquee"
                        android:ellipsize="marquee"
                        android:gravity="center_vertical"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:scrollHorizontally="true"
                        android:singleLine="true"
                        android:textColor="@color/c_content"
                        android:textSize="@dimen/f_normal" />

                    <LinearLayout
                        android:layout_width="@dimen/icon_status_connect_w"
                        android:layout_height="@dimen/icon_status_connect_h"
                        android:orientation="vertical" >

                        <ImageView
                            android:id="@+id/img_status_m"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:background="@drawable/icon_status_m" />

                        <ImageView
                            android:id="@+id/img_connect_c"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:background="@drawable/icon_connect_c" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/li_data_symbol"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/padding_default"
                android:visibility="visible" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:gravity="top" >

                    <com.app.custom.CustomTextView
                        android:id="@+id/tv_name_initial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/c_content"
                        android:textSize="@dimen/f_normalmor"
                        android:textStyle="bold" />

                    <com.app.custom.CustomTextView
                        android:id="@+id/tv_invest_value1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:gravity="right"
                        android:textColor="@color/c_success"
                        android:textSize="@dimen/f_normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/watchlist_detail_margin_top"
                    android:paddingBottom="@dimen/padding_default" >

                    <com.app.custom.CustomTextView
                        android:id="@+id/tv_name_t"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.95"
                        android:textColor="@color/c_content"
                        android:textSize="@dimen/f_symbol_detail" />

                    <com.app.custom.CustomTextView
                        android:id="@+id/tv_invest_change"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:textColor="@color/c_success"
                        android:textSize="@dimen/f_symbol_detail" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/li_data"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_default"
            android:orientation="vertical"
            android:padding="@dimen/padding_default" >

            <com.app.bids.SlidingPanel
                android:id="@+id/popup_window_detail"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="visible" >

                <LinearLayout
                    android:id="@+id/li_data_all"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/bg_default"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal"
                        android:weightSum="3" >

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.3"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="@dimen/padding_row" >

                            <ImageView
                                android:id="@+id/img_icon"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:scaleType="fitXY"
                                android:src="@drawable/bg_circle" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2.7"
                            android:orientation="vertical" >

                            <com.app.custom.CustomTextView
                                android:id="@+id/tv_asset_name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.95"
                                android:textColor="@color/c_content"
                                android:textSize="@dimen/f_small" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Rating"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_small" />

                                <RatingBar
                                    android:id="@+id/rb_rating"
                                    style="?android:attr/ratingBarStyleSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:clickable="false"
                                    android:focusable="false"
                                    android:isIndicator="true"
                                    android:numStars="5"
                                    android:progressTint="@color/c_warning"
                                    android:stepSize="1.0" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/li_attr_symbol"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_default"
                        android:weightSum="2" >

                        <LinearLayout
                            android:id="@+id/col_1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingRight="@dimen/padding_default" >

                            <LinearLayout
                                android:id="@+id/li_asset_value"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.8"
                                    android:singleLine="true"
                                    android:text="The NAV."
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_asset_value"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.2"
                                    android:gravity="right"
                                    android:singleLine="true"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_watchlist_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_invest_value"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.8"
                                    android:singleLine="true"
                                    android:text="Investment"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_invest_value"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.2"
                                    android:gravity="right"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_sell_price"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.8"
                                    android:singleLine="true"
                                    android:text="Bid"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_sell_price"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.2"
                                    android:gravity="right"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_buyback_price"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.8"
                                    android:singleLine="true"
                                    android:text="Offer"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_buyback_price"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.2"
                                    android:gravity="right"
                                    android:textColor="@color/c_blue"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/col_2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:paddingLeft="@dimen/padding_default" >

                            <LinearLayout
                                android:id="@+id/li_fund_category"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:text="Fund type"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail_small" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_fund_category"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_dividend_policy"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:text="Dividend"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_dividend_policy"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textColor="@color/c_blue"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_register_date"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:text="Inception"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_register_date"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />

                            <LinearLayout
                                android:id="@+id/li_date_at"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:paddingBottom="@dimen/padding_row"
                                android:paddingTop="@dimen/padding_row" >

                                <com.app.custom.CustomTextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:text="As at"
                                    android:textColor="@color/c_title"
                                    android:textSize="@dimen/f_symbol_detail" />

                                <com.app.custom.CustomTextView
                                    android:id="@+id/tv_date_at"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textColor="@color/c_content"
                                    android:textSize="@dimen/f_symbol_detail" />
                            </LinearLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/line_wh"
                                android:background="@color/c_line" />
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/padding_default" />

                    <LinearLayout
                        android:id="@+id/li_buysell"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/padding_default" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="2" >

                            <com.app.custom.CustomTextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Low risk"
                                android:textColor="@color/c_success"
